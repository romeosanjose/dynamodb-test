{
    "name": "php-dynamodb-app",
    "description": "A PHP application to read and write to AWS DynamoDB",
    "require": {
        "aws/aws-sdk-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}